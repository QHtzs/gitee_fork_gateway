<?xml version="1.0" encoding="UTF-8"?>

<!-- ports config 端口配置 G:GateWay, W: WEB, C: APP -->
<Config>

<Wp>8001</Wp>
<Gp>8000</Gp>
<Cp>9000</Cp>

<!-- need Starts:是否启动 -->
<Ws>true</Ws>
<Gs>true</Gs>
<Cs>true</Cs>

<!-- Encrypt 是否加密-->
<We>false</We>
<Ge>true</Ge>
<Ce>false</Ce>

<!-- routine number:并发开启客户端个数,仅启动配置为false时有效 -->
<Wn>1000</Wn>
<Gn>1000</Gn>
<Cn>1000</Cn>

<!-- Id from: ID起始生成 -->
<Wf>1</Wf>
<Gf>1</Gf>
<Cf>1</Cf>

<!-- Confirm Format-%d:digit: 认证的内容格式, 若长度不为3的倍数，程序会填充' '自动对齐为3的倍数 -->
<ConfirmFmt>TTG_DEMO_%d</ConfirmFmt>


<!--是否发送消息-->
<Wmsg>false</Wmsg>
<Gmsg>true</Gmsg>
<Cmsg>false</Cmsg>

<!-- 发送消息的时间间隔: 秒-->
<Inteval>2</Inteval>


</Config>




